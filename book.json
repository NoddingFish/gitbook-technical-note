{
  "title" : "技术笔记 · Gitbook",
  "author" : "打盹的鱼",
  "description" : "记录平时工作和学习中遇到的一些注意点",
  "language" : "zh-hans",
  "gitbook" : "3.2.3",
  "links" : {
    "sidebar" : {
      "个人网站" : "http://www.whxuran.cn"
    }
  },
  "theme-default": {
    "showLevel": true
  },
  "plugins": [
    "expandable-chapters-small",
    "theme-comscore",
    "github",
    "search-pro",
    "-search",
    "-lunr",
    "back-to-top-button",
    "flexible-alerts",
    "include-codeblock",
    "ace"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/NoddingFish"
    },
    "flexible-alerts": {
      "style": "flat",
      "comment": {
        "label": "Comment",
        "icon": "fa fa-comments",
        "className": "info"
      }
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": false,
      "theme": "coffee"
    }
  }
}